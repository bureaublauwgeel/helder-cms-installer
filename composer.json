{
    "name": "bureaublauwgeel/helder-cms-installer",
    "type": "installer",
    "description": "Helder CMS installer",
    "homepage": "http://www.bureaublauwgeel.nl",
    "authors": [
        {
            "name": "Ray Kootstra",
            "email": "r.kootstra@bureaublauwgeel.nl"
        }
    ],
    "keywords" : [
        "helder cms installer"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
        "php": ">=5.5",
        "symfony/console": "2.8.*",
        "symfony/process": "2.8.*"
    },
    "repositories" : [{
        "type" : "vcs",
        "url" : "https://github.com/bureaublauwgeel/helder-cms-installer"
    }],
    "extra": {
        "installer-paths": {
            "/": ["vendor/package"]
        }
    },
    "version": "1.0.6",
    "minimum-stability": "stable",
    "prefer-stable": true
}
